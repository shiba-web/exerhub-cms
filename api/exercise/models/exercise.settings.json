{
  "kind": "collectionType",
  "collectionName": "exercises",
  "info": {
    "name": "Exercise",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": [
      "createdAt",
      "updatedAt"
    ],
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "content": {
      "type": "richtext",
      "required": true
    },
    "answer": {
      "type": "richtext"
    },
    "difficulty": {
      "required": false,
      "unique": false,
      "private": false,
      "default": 1,
      "min": 1,
      "max": 10,
      "type": "integer"
    },
    "categories": {
      "via": "exercises",
      "collection": "category"
    },
    "paper": {
      "via": "exercises",
      "model": "paper"
    }
  }
}
